/dts-v1/;


#include "acq400_common.dtsi"
#include "acq400_axi_dma.dtsi"

&qspi {
	is-dual = <1>;
	xlnx,qspi-mode = <0x2>;
};

&i2c0 {
	status = "okay";
	
	i2c-reset = <&gpio0 46 0>;
	i2c0sw73: i2cswitch@73 {
		compatible = "nxp,pca9548";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0x73>;
		
		mb_i2c: i2c@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
			
			/* uRTM */
			elves: gpio@21 {
				compatible="nxp,pca9534";
				reg=<0x21>;				
			};
			hwmon@28 {
				compatible="ad,ad7417";
				reg=<0x28>;
			};
			hwmon@29 {
				compatible="ad,ad7417";
				reg=<0x29>;
			};
			
		};

		elf1: i2c@1 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0x1>;
	
			eeprom@50 {
				compatible = "at,24c64";
				reg = <0x51>;
			};
			hwmon@28 {
				compatible="ad,ad7417";
				reg=<0x29>;
			};
		};
		elf2: i2c@2 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0x2>;
		
			eeprom@50 {
				compatible = "at,24c64";
				reg = <0x52>;
			};
			hwmon@28 {
				compatible="ad,ad7417";
				reg=<0x2a>;
			};
		};
		elf3: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0x3>;
		};
	};
};

