/dts-v1/;
/plugin/;	/* allow undefined label references and record them */

/ {
	fragment@0 {
		target = <&spi0>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;			
			status = "okay";

			ad4858_1A: adc@0{		
/*                
                refin-supply = <&refin>;
                clocks = <&ref_clk>;
                clock-names = "scki";
                dmas = <&rx_dma 0>;
                dma-names = "rx";
                pwms = <&axi_pwm_gen 0 0>;
                pwm-names = "cnv";
*/
				reg = <0x10>;
				spi-max-frequency = <10000000>;
				compatible = "adi,ad4858";
			};
			ad4858_1B: adc@1{
				reg = <0x11>;

				spi-max-frequency = <10000000>;
				compatible = "adi,ad4858";
			};
			ad4858_1C: adc@2{
				reg = <0x12>;

				spi-max-frequency = <10000000>;
				compatible = "adi,ad4858";
			};
			ad4858_1D: adc@3{
				reg = <0x13>;

				spi-max-frequency = <10000000>;
				compatible = "adi,ad4858";
			};			
			ad4858_1X: adc@f{
				reg = <0x1F>;

				spi-max-frequency = <10000000>;
				compatible = "adi,ad4858";
			};
		};
	};
};